<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ServiceController">
   <children>
      <Button layoutX="27.0" layoutY="35.0" mnemonicParsing="false" onAction="#showService" text="Show Service" />
      <TableView fx:id="tableService" layoutX="22.0" layoutY="86.0" prefHeight="288.0" prefWidth="695.0">
        <columns>
          <TableColumn fx:id="title" prefWidth="173.0" text="Title" />
          <TableColumn fx:id="price" prefWidth="122.0" text="Price" />
            <TableColumn fx:id="description" prefWidth="399.0" text="Description" />
        </columns>
      </TableView>
      <Button layoutX="145.0" layoutY="35.0" onAction="#addService" text="Add Service" />
      <Button layoutX="255.0" layoutY="35.0" onAction="#updateDelete" text="Update et delete" />
      <Button fx:id="backBtn" layoutX="46.0" layoutY="410.0" mnemonicParsing="false" onAction="#back" text="Back" />
   </children>
</AnchorPane>
