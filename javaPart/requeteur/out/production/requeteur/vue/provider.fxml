<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ProviderController">
   <children>
      <Button layoutX="27.0" layoutY="35.0" mnemonicParsing="false" onAction="#showProvider" text="Show Provider" />
      <TableView fx:id="tableProvider" layoutX="22.0" layoutY="86.0" prefHeight="288.0" prefWidth="763.0">
        <columns>
          <TableColumn fx:id="firstName" prefWidth="92.0" text="FirstName" />
          <TableColumn fx:id="lastName" prefWidth="91.0" text="LastName" />
            <TableColumn fx:id="mail" prefWidth="163.0" text="Mail" />
            <TableColumn fx:id="company" prefWidth="111.0" text="Company" />
            <TableColumn fx:id="address" prefWidth="193.0" text="Address" />
            <TableColumn fx:id="city" prefWidth="111.0" text="City" />
        </columns>
      </TableView>
      <Button layoutX="145.0" layoutY="35.0" onAction="#addProvider" text="Add Provider" />
      <Button layoutX="255.0" layoutY="35.0" onAction="#updateDelete" text="Update et delete" />
      <Button fx:id="backBtn" layoutX="46.0" layoutY="410.0" mnemonicParsing="false" onAction="#back" text="Back" />
   </children>
</AnchorPane>
